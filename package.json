{
	"name": "directus-extension-simple-autosync",
	"description": "An extension for pulling and pushing snapshot configurations between Directus environments.",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-bundle"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/digi-regeringen-ax/directus-extension-simple-autosync"
	},
	"type": "module",
	"files": [
		"dist"
	],
	"engines": {
		"node": "18.20.2"
	},
	"directus:extension": {
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "hook",
				"name": "pullpush",
				"source": "src/index.js"
			}
		],
		"host": "^11.3.2"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"add": "directus-extension add"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "12.1.4"
	}
}
